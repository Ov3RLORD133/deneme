# KeyChaser Configuration Example
# Copy this file to .env and adjust values as needed

# Server Configuration
KEYCHASER_HOST=0.0.0.0
KEYCHASER_API_PORT=8000

# Database
KEYCHASER_DB_PATH=data/keychaser.db

# Logging
KEYCHASER_LOG_PATH=data/logs
KEYCHASER_LOG_LEVEL=INFO
KEYCHASER_LOG_TO_FILE=true

# Protocol Handlers
# Comma-separated list of enabled protocol modules
KEYCHASER_ENABLED_PROTOCOLS=["example_logger"]

# Security Settings
KEYCHASER_MAX_PACKET_SIZE=65536
KEYCHASER_CONNECTION_TIMEOUT=30
KEYCHASER_MAX_CONNECTIONS_PER_IP=10

# Dashboard
KEYCHASER_DASHBOARD_REFRESH_INTERVAL=5

# Telegram Notifications
# Get bot token from @BotFather on Telegram
# Get chat ID by messaging your bot and visiting: https://api.telegram.org/bot<YOUR_TOKEN>/getUpdates
KEYCHASER_TELEGRAM_BOT_TOKEN=
KEYCHASER_TELEGRAM_CHAT_ID=

# Threat Intelligence APIs
# VirusTotal: Get API key from https://www.virustotal.com/gui/my-apikey
# AbuseIPDB: Get API key from https://www.abuseipdb.com/account/api
KEYCHASER_VIRUSTOTAL_API_KEY=
KEYCHASER_ABUSEIPDB_API_KEY=

# Debug Mode (set to false in production)
KEYCHASER_DEBUG=false
